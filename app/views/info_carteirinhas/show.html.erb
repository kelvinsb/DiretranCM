<div class="col-sm-12">
    <%- model_class = InfoCarteirinha -%>
    <div class="page-header">
      <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
    </div>
    
    <dl class="dl-horizontal">
      <dt><strong><%= model_class.human_attribute_name(:nome_tipo_estacionamento) %>:</strong></dt>
      <dd><%= @info_carteirinha.nome_tipo_estacionamento %></dd>
      <dt><strong><%= model_class.human_attribute_name(:resolucao) %>:</strong></dt>
      <dd><%= @info_carteirinha.resolucao %></dd>
      <dt><strong><%= model_class.human_attribute_name(:uf) %>:</strong></dt>
      <dd><%= @info_carteirinha.uf %></dd>
      <dt><strong><%= model_class.human_attribute_name(:municipio) %>:</strong></dt>
      <dd><%= @info_carteirinha.municipio %></dd>
      <dt><strong><%= model_class.human_attribute_name(:orgao_expedidor) %>:</strong></dt>
      <dd><%= @info_carteirinha.orgao_expedidor %></dd>
      <dt><strong><%= model_class.human_attribute_name(:nome_diretor) %>:</strong></dt>
      <dd><%= @info_carteirinha.nome_diretor %></dd>
      <dt><strong><%= model_class.human_attribute_name(:orgao_exp_diretor) %>:</strong></dt>
      <dd><%= @info_carteirinha.orgao_exp_diretor %></dd>
      <dt><strong><%= model_class.human_attribute_name(:regras_utilizacao) %>:</strong></dt>
      <dd><%= @info_carteirinha.regras_utilizacao %></dd>
      <dt><strong><%= model_class.human_attribute_name(:numeracao_carteirinha) %>:</strong></dt>
      <dd><%= @info_carteirinha.numeracao_carteirinha %></dd>
    </dl>
    
    <%= link_to t('.back', :default => t("helpers.links.back")),
                  info_carteirinhas_path, :class => 'btn btn-secondary'  %>
    <%= link_to t('.edit', :default => t("helpers.links.edit")),
                  edit_info_carteirinha_path(@info_carteirinha), :class => 'btn btn-primary' %>
    <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                  info_carteirinha_path(@info_carteirinha),
                  :method => 'delete',
                  :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                  :class => 'btn btn-danger' %>
    
</div>