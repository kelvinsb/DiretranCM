
<div class="col-sm-12">
    <div class="form-login">
      <h2><%=I18n.t'log_sess_new'%> </h2>
      <%= devise_error_messages! %>
        <br>
        <br>

      <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
        <div id="userName" class="form-group">
          <%= f.label :cpf, "CPF" %>
          <%= f.text_field :cpf, autofocus: true, :class => 'form-control' %>
        </div>
        <br />
        <div id="userPassword" class="form-group">
          <%= f.label :password, "Senha" %>
          <%= f.password_field :password, autocomplete: "off", :class => 'form-control' %>
        </div>

        <% if devise_mapping.rememberable? -%>
          <div>
            <%= f.check_box :remember_me %>
            <%= f.label :remember_me, "Lembrar senha" %>
          </div>
        <% end -%>

        <div class="wrapper">
        <span class="group-btn"> 

        <div class="actions" >
          <%= f.submit "Entrar", class: "btn btn-primary" %>
          <%= link_to "Cadastrar",  new_usuario_registration_path , class: 'btn btn-secondary' %>
        </div>
      <% end %>
      </span>
      </div>
    </div>
</div>

               
    

