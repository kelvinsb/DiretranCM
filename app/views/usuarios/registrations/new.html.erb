<div class="col-sm-12">
    <h2><%=I18n.t'sign_reg_new'%></h2>

    <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
      
      <div class="form-group">
        <%= f.label :cpf, "CPF" %><br />
        <%= f.text_field :cpf, autofocus: true, :class => 'form-control' %>
      </div>
    
      <div class="form-group">
        <%= f.label :password, "Senha" %>
        <% if @minimum_password_length %>
        <em>(<%= @minimum_password_length %> <%=I18n.t'character_edit_pass'%>)</em>
        <% end %><br />
        <%= f.password_field :password, autocomplete: "off", :class => 'form-control' %>
      </div>
    
      <div class="form-group">
        <%= f.label :password_confirmation, "Confirmação de senha" %><br />
        <%= f.password_field :password_confirmation, autocomplete: "off", :class => 'form-control' %>
      </div>
    
      <div class="actions">
        <%= f.submit "Cadastrar", :class => 'btn btn-primary' %>
        <%= link_to "Autenticar",  new_usuario_session_path , class: 'btn btn-secondary' %>
      </div>
    <% end %>
    
</div>